<div class="well">
<h4>THIS IS MY FIRST OFFICIAL WEBSITE</h4>
<p>Do you have any questions for me?</p>
</div>

<div class="container">
  <% @questions.each do |q| %>
<div class="media">
  <div class="media-left">
    <a href="#">
      <img class="media-object" src="http://www.megaicons.net/static/img/icons_sizes/368/1041/64/letter-m-icon.png">
    </a>
  </div>
  <div class="media-body">
    <h4 class="media-heading">   <%= q.email %>asked</h4>
    <div>
      <%= time_ago_in_words q.created_at %>ago
  </div>

    <%= q.body %>
<div>
    <a href="<%= question_path(q)%>" class="btn btn-success btn-xs">view answers</a>
</div>
  </div>
</div>



<% end %>

<% if @questions.empty? %>
<div class="alert alert-info">nothing is here,be the first to ask?</div>
<% end %>









</div>